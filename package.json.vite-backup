  "scripts": {
    "dev": "NODE_ENV=development vite",
    "dev:server": "cd server && npm run dev",
    "dev:all": "npm run dev & npm run dev:server",
    "build": "npm run build:server && npm run build:frontend",
    "build:server": "cd server && npm install --production",
    "build:frontend": "NODE_ENV=production vite build",
    "build:dev": "NODE_ENV=development vite build",
    "build:analyze": "NODE_ENV=production ANALYZE=true vite build",
    "vercel-build": "npm run build:server && npm run build:frontend",
    "build:vercel": "npm run vercel-build",
    "start": "cd server && npm start",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "db:init": "cd server && node src/config/init-db.js",
    "db:seed": "cd server && node --input-type=module -e \"import seedDatabase from './src/config/seed.js'; seedDatabase().then(() => console.log('âœ… Seeding completed!')).catch(console.error)\"",
    "install:all": "npm install && cd server && npm install",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:all": "npm run optimize:images && npm run build:frontend",
    "build:optimized": "npm run optimize:images && NODE_ENV=production vite build --config vite.config.optimized.ts",
    "performance:audit": "lighthouse http://localhost:3000 --output=json --output-path=./performance-audit.json",
    "playwright:install": "npx playwright install --with-deps",
    "test:visual": "playwright test --project=visual-regression",
    "test:visual:headed": "playwright test --project=visual-regression --headed",
    "test:design-capture": "playwright test --project=design-capture --headed",
    "test:design-validation": "npm run test:design-capture && npm run test:visual && npm run audit:design-system",
    "test:component-visual": "playwright test --grep component-visual",
    "test:mobile": "playwright test --project=mobile",
    "test:desktop": "playwright test --project=desktop",
    "audit:design-system": "node scripts/design-system-audit.js",
    "update:visual-baselines": "playwright test --project=visual-regression --update-snapshots",
    "ai:design-iteration": "node scripts/ai-design-iteration.js"
  },
